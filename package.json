{
  "name": "batch-rename",
  "version": "1.0.0",
  "description": "一个批量文件重命名工具，基于强大的 manoco 编辑器而非其他复杂的规则",
  "main": "./out/main/index.js",
  "author": "rxliuli",
  "homepage": "https://github.com/rxliuli",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "electron-updater": "^5.3.0"
  },
  "prettier": "@liuli-util/prettier-standard-config",
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@liuli-util/prettier-standard-config": "^0.3.0",
    "@liuli-util/vite-plugin-css-dts": "^0.2.0",
    "@monaco-editor/react": "^4.5.1",
    "@picocss/pico": "^1.5.10",
    "@types/file-saver": "^2.0.5",
    "@types/node": "16.18.21",
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "classnames": "^2.3.2",
    "electron": "^22.3.4",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.21",
    "file-saver": "^2.0.5",
    "is-electron": "^2.2.2",
    "jszip": "^3.10.1",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "monaco-editor": "^0.38.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-use": "^17.4.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.1"
  }
}
